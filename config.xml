<?xml version='1.0' encoding='utf-8'?>
<widget id="com.actualsoft.jmap" version="1.0.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Mis Prácticas</name>
    <description>
        Una aplicación libre para que puedas mejorar tus entrenamientos!
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    
    <platform name="android">
  <!--   <config-file target="AndroidManifest.xml" parent="/manifest" xmlns:android="http://schemas.android.com/apk/res/android">
  <uses-permission android:name="android.permission.INTERNET"/>
  <uses-permission android:name="android.permission.ACCESS_BACKGROUND_LOCATION" />            
        <uses-permission android:name="com.google.android.gms.permission.ACTIVITY_RECOGNITION" />
</config-file> -->
        
    <icon src="src/assets/icon.png" density="ldpi" />
              <icon src="src/assets/icon.png" density="mdpi" />
        <icon src="src/assets/icon.png" density="hdpi" />
        <icon src="src/assets/icon.png" density="xhdpi" />
        <preference name="android-minSdkVersion" value="24"/>
        <preference name="android-targetSdkVersion" value="30"/>
        <allow-intent href="market:*" />
          
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>


    
</widget>
